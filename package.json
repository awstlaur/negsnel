{
    "name": "negsnel",
    "version": "1.0.0",
    "description": "Negative Snell's Law Simulator",
    "main": "docs/index.html",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "precommit": "yarn run eslint && yarn run build:prod && git status",
        "eslint": "eslint --ext .js,.ts,.jsx,.tsx,.json .",
        "eslint-fix": "eslint --ext .js,.ts,.jsx,.tsx,.json --fix .",
        "build:prod": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "start:dev": "webpack-dev-server --watch --config webpack.dev.js",
        "start:prod": "webpack-dev-server --watch --config webpack.prod.js",
        "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/awstlaur/negsnel.git"
    },
    "keywords": [
        "math",
        "geometry",
        "education",
        "simulation",
        "tiling",
        "snell"
    ],
    "author": "Alexander St Laurent",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/awstlaur/negsnel/issues"
    },
    "homepage": "https://github.com/awstlaur/negsnel#readme",
    "dependencies": {
        "jquery": "^3.0.0",
        "mousetrap": "1.4.6",
        "raphael": "^2.2.8"
    },
    "devDependencies": {
        "@babel/core": "^7.1.5",
        "@babel/preset-env": "^7.1.5",
        "@types/jquery": "^3.3.33",
        "@types/mousetrap": "^1.6.3",
        "@types/raphael": "^2.1.30",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "babel-loader": "^8.0.4",
        "bootstrap": "3.4.1",
        "css-loader": "^2.1.1",
        "eslint": "^5.9.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-json": "^2.1.1",
        "eslint-plugin-prettier": "^3.0.1",
        "file-loader": "^3.0.1",
        "style-loader": "^0.23.1",
        "ts-loader": "^5.3.3",
        "typescript": "^3.4.1",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.1.4"
    }
}
