{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "mousetrap": ["node_modules/mousetrap/mousetrap"],
            "raphael": ["node_modules/raphael/raphael"]
